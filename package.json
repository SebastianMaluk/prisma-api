		"@prisma/client": "^4.14.0",
		"@trpc/client": "^10.26.0",
		"@trpc/server": "^10.26.0",
		"dotenv": "^16.0.3",
		"fastify": "^4.17.0",
		"superjson": "^1.12.3",
		"trpc-openapi": "^1.1.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/cors": "^2.8.13",
		"@types/node": "^20.1.7",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.8",
		"prisma": "^4.14.0",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsx": "^3.12.7",
		"typescript": "^5.0.4"
	}
